@model ChangTing.Users.Models.UsersInfo
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" type="text/css" href="~/Content/UI/css/Login.css" />
    <title>用户登录</title>
</head>
<body>
@using (Html.BeginForm("Login", "UI", FormMethod.Post))
{
    <div>
        <div class="cc">
            <a href="index">首页</a>
            <a href="register">注册</a>
        </div>

        <h1>欢迎进入畅听 </h1>
        <div class="neibu">
            <div class="login-one">
                登录名：
                <input type="text"  name="Login-Name" value="@Model.LoginName" class="txt Name"  /><br /> 
                密　码：
                <input type="password"  name="Login-Password" value="@Model.PassWord" class="txt Password" /><br />
                <br />
                
            </div>

            <div class="login-two">
                <input type="submit" name="" id="" value="登录" />
                <input type="reset"  value="重置" />
            </div>
        </div>

    </div>
}
</body>
</html>
